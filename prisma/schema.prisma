generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lect {
  id     Int    @id @default(autoincrement())
  tag    String @unique
  name   String
  imgSrc String
}

model Review {
  id          Int      @id @default(autoincrement())
  authorId    String
  reviews     Int[]
  comments    String?
  lecturerTag String
  createdAt   DateTime @default(now())
  kelas       String
  likeIds     String[] @default([])
  dislikeIds  String[] @default([])
}
